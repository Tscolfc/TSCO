pragma solidity ^0.5.17;

// SPDX-License-Identifier: GPL-3.0
contract CheckIn {
    mapping(address => mapping(uint256 => bool)) public checkInData;

    function checkIn(uint256 date) external  {
        checkInData[msg.sender][date] = true;
    }
}
